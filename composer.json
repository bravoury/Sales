{
    "name": "tadascript/sales",
    "description": "Sales package for tadascript.",
    "keywords": ["lavalite", "Sales"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lavalite",
            "email": "info@lavalite.org"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds"
        ],
        "psr-4": {
            "Tadascript\\Sales\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tadascript\\Sales\\Providers\\SalesServiceProvider"
            ],
            "aliases": {
                "Blog"       : "Tadascript\\Sales\\Facades\\Sales"
            }
        }
    },
    "minimum-stability": "dev"
}
